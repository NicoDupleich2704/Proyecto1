<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#FFFFFF">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:src="@drawable/fondo_alimentos" />

    <ImageButton
        android:id="@+id/botonLacteos"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/tipo1"
        android:onClick="ingresarLacteos" />

    <ImageButton
        android:id="@+id/botonPatatas"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/tipo3" />

    <ImageButton
        android:id="@+id/botonCarnesPescadoHuevo"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="190dp"
        android:background="@drawable/tipo2" />

    <ImageButton
        android:id="@+id/botonVerduras"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="190dp"
        android:background="@drawable/tipo4" />

    <ImageButton
        android:id="@+id/botonFrutas"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="370dp"
        android:background="@drawable/tipo5" />

    <ImageButton
        android:id="@+id/botonCereales"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="370dp"
        android:background="@drawable/tipo6" />

    <ImageButton
        android:id="@+id/botonBebidas"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="145dp"
        android:layout_marginTop="550dp"
        android:background="@drawable/tipo7" />

    <TextView
        android:id="@+id/txt_Lechederivados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/botonLacteos"
        android:layout_marginStart="30dp"
        android:layout_marginTop="0dp"
        android:text="@string/txt_LechesDerivados"
        android:background="#FFFFFF"
        android:textAlignment="center"
        android:fontFamily="serif-monospace"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/txt_patatas"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/botonPatatas"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-20dp"
        android:fontFamily="serif-monospace"
        android:background="#FFFFFF"
        android:textColor="#000000"
        android:text="@string/txt_PatatasLegumbresFrutosSecos"
        android:textAlignment="center"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/txt_Carnes"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:fontFamily="serif-monospace"
        android:textColor="#000000"
        android:layout_marginStart="45dp"
        android:background="#FFFFFF"
        android:layout_below="@+id/botonCarnesPescadoHuevo"
        android:textAlignment="center"
        android:text="@string/txt_CarnePescadoHuevo" />

    <TextView
        android:id="@+id/txtVerduras"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="serif-monospace"
        android:textColor="#000000"
        android:layout_below="@+id/botonVerduras"
        android:layout_marginStart="245dp"
        android:layout_marginTop="0dp"
        android:background="#FFFFFF"
        android:textAlignment="center"
        android:text="@string/txt_VerdurasHortalizas" />

    <TextView
        android:id="@+id/txt_frutas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/botonFrutas"
        android:textAlignment="center"
        android:layout_marginStart="85dp"
        android:fontFamily="serif-monospace"
        android:background="#FFFFFF"
        android:textColor="#000000"
        android:text="@string/txt_Frutas" />

    <TextView
        android:id="@+id/txt_Cereales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:layout_below="@+id/botonCereales"
        android:fontFamily="serif-monospace"
        android:textColor="#000000"
        android:background="#FFFFFF"
        android:layout_marginStart="245dp"
        android:text="@string/txt_CerealesDerivados" />

    <TextView
        android:id="@+id/txt_bebidas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="serif-monospace"
        android:textColor="#000000"
        android:textAlignment="center"
        android:layout_marginStart="180dp"
        android:background="#FFFFFF"
        android:layout_below="@+id/botonBebidas"
        android:text="@string/txt_Bebidas" />

</RelativeLayout>